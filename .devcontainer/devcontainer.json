{
  "image": "node:14",
  "hostRequirements": {
    "cpus": 2
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "python -m SimpleHTTPServer 8000",
  "postCreateCommand": "",
  "postAttachCommand": "",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "index.html"
      ]
    }
  },
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [3000]
}
